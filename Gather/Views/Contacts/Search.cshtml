@using Gather.Models

    @foreach (Contact contact in Model)
    {
        <b>First Name: </b> @contact.FirstName<br />
        <b>Last Name: </b> @contact.LastName<br />
        <b>Email Address: </b> @contact.Email<br />
        <b>Organization: </b>  @contact.CompanyName<br />
        <b>Location: </b> @contact.Location<br />
        <b>Department:</b>  @contact.Department<br />
        
    }

@{
  Layout = "_Layout";
}
